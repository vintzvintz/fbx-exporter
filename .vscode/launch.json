{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Freebox Exporter",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}",
			"args": ["-hostDetails", "secrets/freebox_token.json"],
			"cwd": "${workspaceFolder}/dev",
			"env": {
				"FBX_APPNAME": "freebox-exporter-dev"
			},
			"console": "integratedTerminal",
			"showLog": true
		},
		{
			"name": "Launch Freebox Exporter (Debug)",
			"type": "go",
			"request": "launch",
			"mode": "debug",
			"program": "${workspaceFolder}",
			"args": ["-hostDetails", "secrets/freebox_token.json"],
			"cwd": "${workspaceFolder}/dev",
			"env": {
				"FBX_APPNAME": "freebox-exporter-dev",
				"GOMAXPROCS": "1"
			},
			"console": "integratedTerminal",
			"showLog": true,
			"trace": "verbose"
		},
		{
			"name": "Launch Freebox Exporter (Verbose Debug)",
			"type": "go",
			"request": "launch",
			"mode": "debug",
			"program": "${workspaceFolder}",
			"args": ["-debug", "-hostDetails", "secrets/freebox_token.json"],
			"cwd": "${workspaceFolder}/dev",
			"env": {
				"FBX_APPNAME": "freebox-exporter-dev",
				"GOMAXPROCS": "1"
			},
			"console": "integratedTerminal",
			"showLog": true,
			"trace": "verbose"
		},
		{
			"name": "Attach to Process",
			"type": "go",
			"request": "attach",
			"mode": "local",
			"processId": 0
		}
	]
}